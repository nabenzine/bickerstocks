}
ul.tabs li a.active { background: none repeat scroll 0 0 #FFFFFF;color: {{ settings.color_primary }}/*FY #6B40A4*/ ;height: 44px;position: relative;}


ul.tabs:before, ul.tabs:after {
  content: " ";
  display: block;
  height: 0;
  overflow: hidden;
  visibility: hidden;
  width: 0;
}
ul.tabs:after {
  clear: both;
}

