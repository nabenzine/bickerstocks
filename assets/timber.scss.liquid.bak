  text-align: center;
  margin:0 0 0 34px;
  max-width: 100%;

  a {
    color: $colorNavText;
    max-width: {{ settings.logo_max_width_2ry }}px;
    margin: 0 auto;
  }

  a,
  a:hover,
  a:focus {
    text-decoration: none;
  }

  a, img {
    display: block;
  }

  img {
    margin: 0 auto;
  }
}

.site-header--text-links {
  margin-bottom: 0;
}

/*============ If logo 2ry is on the same line as navigation ============ */
{% unless settings.nav_below_logo_2ry or settings.show_header_lines == false %}
  .site-header_2ry .grid--full {
    border-bottom: 1px solid $colorBorder;
