/*================ Module-specific styles ================*/
/*================ Module | Breadcrumbs and Tag Sorting ================*/
.breadcrumb,
.tags {
  color: $colorPrimary;

  a {
    color: #999999;

    &:hover,
    &:focus,
    &:active {
      color: $colorPrimary;
    }
  }
}

.breadcrumb {
  margin-bottom: $gutter / 2.5;
  color: {{ settings.color_primary }}/*FY #6B40A4 */ ;
  font-family: 'Lato', sans-serif;    font-size: 15px;
    font-weight: 400;

  a,
  span {
    display: inline;
    padding-right: 5px;
    margin-right: 5px;

    &:first-child {
      padding-left: 0;
    }
  }
