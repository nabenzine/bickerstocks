    display: block;
    table-layout: fixed;
    width: 100%;

    .grid__item {
      display:block;
      vertical-align: middle;
      float: left;
    }
  }
}

.cart__image {
  display: block;

  img {
    display: block;
  }
}

.cart__subtotal {
  margin: 0 0 0 ($gutter / 3);
  display: inline;
}

.cart__mini-labels {
  display: block;
  margin: ($gutter / 3) 0;
  font-size: em(12px);

